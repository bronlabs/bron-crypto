    "from four_squares import get_four_squares\n",
    "from random import randrange\n",
    "\n",
    "\n",
    "for i in range(64):\n",
    "    mu = randrange(1, 2**256)\n",
    "    w1, w2, w3, w4 = get_four_squares(mu)\n",
    "    assert mu == (w1 ** 2 + w2 ** 2 + w3 ** 2 + w4 ** 2)\n"
