    "from random import randrange\n",
    "\n",
    "for i in range(1000):\n",
    "    mu = randrange(1, 2**32)\n",
    "    (w1, w2, w3, w4) = get_four_square(mu)\n",
    "    assert (mu == w1 ** 2 + w2 ** 2 + w3 ** 2 + w4 ** 2)"
